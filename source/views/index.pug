extends ./layout.pug
block append css
  - if (settings.envflag !== "production")
    link( href='../../bower_components/materialize/dist/css/materialize.css' rel='stylesheet')

block append js
  - if (settings.envflag !== "production")
    script(src='../../bower_components/materialize/dist/js/materialize.js')

block content
  .container
    h1.center-align= title
    .card-panel
      h4 Examples of Framework CSS
      p Here i use
        a(href="http://materializecss.com") Materialize
      .collection
        a.collection-item(href="./bootstrap.html") bootstrap
        a.collection-item(href="./material.html") material
        a.collection-item(href="./fundation.html" disabled) foundation
        a.collection-item(href="./mui.html") mui
        a.collection-item(href="./angular_router.html") angular router
